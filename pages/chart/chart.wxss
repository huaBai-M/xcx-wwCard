/* pages/contact/contact.wxss */

page {
  background-color: #f1f1f1;
}

.inputRoom {
  width: 100vw;
  height: 16vw;
  overflow: hidden;
  border-top: 1px solid #cdcdcd;
  background-color: #f1f1f1;
  position: fixed;
  bottom: 0;
  display: flex;
  align-items: center;
  z-index: 20;
}

input {
  width: 76vw;
  height: 9.33vw;
  background-color: #fff;
  border-radius: 40rpx;
  margin-left: 2vw;
  padding: 0 3vw;
  font-size: 28rpx;
  color: #444;
}

.leftMsg {
  font-size: 35rpx;
  color: #444;
  line-height: 7vw;
  padding: 2vw 2.5vw;
  background-color: #fff;
  margin-left: -1.6vw;
  border-radius: 10rpx;
  z-index: 10;
  max-width: 64vw;
}

.rightMsg {
  font-size: 35rpx;
  color: #444;
  line-height: 7vw;
  padding: 2vw 2.5vw;
  background-color: #96EB6A;
  margin-right: -1.6vw;
  border-radius: 10rpx;
  z-index: 10;
   max-width: 64vw;
}
.inputRoomAbt{
  font-size: 24rpx;
  background-color:#07c160;
  color: #fff;
}
.inputRoomAbtT{

 position: absolute;
 width: 100%;
height: 16vw;
 top: 0;
 left: 0; 
 z-index: 999;
 opacity: 0;
}
.inputRoomAbtT  button {
    width: 100%;
  height: 100%;
  display: block;
  z-index: 999999;
  line-height: 50px;
}
.history{
  /* position: fixed;
  top: 0; */
  color: #333;
  width: 100%;
  background: #f1f1f1;
  font-size: 28rpx;
  padding: 5px;
  z-index: 99999;
  /* display: none; */
  text-align: center;
  display: block;
}
