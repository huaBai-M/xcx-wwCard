image{
  width: auto;
  height: auto;
}
.proLists {
    margin-top: 35rpx;
    display: flex;
    flex-wrap: wrap;
    padding: 0 3%;
    box-sizing: border-box;
}
.imgPro image{
  height: 104px !important;
}
.proLists .list {
    width: 100%;
    text-align: center;
    /* padding-bottom: 50rpx; */
    position: relative;
}

.proLists .raduis {
    width: 47%;
    height: 84%;
    border-radius: 15rpx;
    overflow: hidden;
    display: inline-block;
    margin: 20rpx 0;
    margin-right: 3%;
    box-shadow: 0 0 20rpx #ccc;
}

.proLists .raduis:nth-child(2n) {
    margin-right: 0;
}

.proLists .raduis:nth-child(1), .proLists .raduis:nth-child(2) {
    margin-bottom: 0;
}
.proLists .raduis1 {
    width: 47%;
    height: 84%;
    border-radius: 25rpx;
    overflow: hidden;
    display: inline-block;
    margin-top: 20rpx;
    box-shadow: 0 0 20rpx #ccc;
}
.imgPro image {
    width: 100%;
}

.imgsView {
    display: flex;
}

.imgsView image {
    width: 32rpx;
    border-radius: 100%;
    display: inline-block;
    margin-left: -10rpx;
    border: 2px solid #fff;
}

.imgsView .imgItem:first-child image {
    margin-left: 0;
}

.textPro {
    display: flex;
    flex-wrap: wrap;
    padding: 0 3%;
    box-sizing: border-box;
}

.textPro .namePro {
    width: 100%;
    text-align: left;
    color: #666;
    font-size: 28rpx;
    font-weight: 600;
    border-bottom: 1rpx solid #e7e7e7;
    height: 60rpx;
    line-height: 60rpx;
}

.textPro .imgsView {
    width: 40%;
    height: 80rpx;
    align-items: center;
    overflow: hidden;
}

.textPro .numPro {
    width: 60%;
    color: #999;
    font-size: 22rpx;
    height: 80rpx;
    display: flex;
    align-items: center;
    padding-left: 10rpx;
    box-sizing: border-box;
    overflow: hidden;
}

.swiper {
    height: 412rpx;
    position: static;
}

.swiper .wx-swiper-dots.wx-swiper-dots-horizontal {
    position: absolute;
    top: 385rpx;
    z-index: 999;
}
.swiper_dot{
  position: relative;
  top: -50rpx;
}
.swiper_dot .dot {
    width: 16rpx;
    display: inline-flex;
    height: 16rpx;
    margin-left: 12rpx;
    justify-content: space-between;
     background: #e5e5e5;
    border-radius: 12rpx;
    -webkit-border-radius: 12rpx;
    -moz-border-radius: 10rxp;
}
.swiper_dot .active {
    width: 30rpx;
    background: #006cff;
}