    width: 30rpx;
    height: 30rpx;
    border-radius: 100%;
    line-height: 30rpx;
    text-align: center;
}
.btns {
    width: 90%;
    margin: 0 auto;
    background-color: #fff;
    display: flex;
    height: 80rpx;
    align-items: center;
    border-radius: 0 0 25rpx 25rpx;
    box-shadow: 0 0 20rpx #e7f2ff;
}
.btns .shareBtn {
    flex: 1;
    text-align: center;
    border-right: 1rpx solid #e7e7e7;
    font-size: 30rpx;
    color: #666;
    line-height: normal;
    border: none;
    background: transparent;
}
.shareBtn::after,.shareBtn::before {
    border:none;
    display: none;
}
.btns .delete {
    flex: 1;
    text-align: center;
    font-size: 30rpx;
    color: #666;
}
.btns .iconfont {
    color: #cbced2;
    font-size: 30rpx;
    margin-right: 10rpx;
}
.btns .icon-fenxiang {
    color: #84db02
}
.delete .delId {
    color: #f46000;
}
.btns .icon-fenzutongji{
  color: #8d64fe;
}
.icon-gongsimingcheng {
