/* pages/callLists/callLists.wxss */
@import "../../font/iconfont.wxss";
page {
    width: 100%;
    height: 100%;
}
whole{
  width: 100%;
  height: 100%;
  overflow: auto;
}

.pageBody {
    width: 100%;
    min-height: 100%;
    background-color: #ecf7ff;
    padding: 0 4%;
    box-sizing: border-box;
    padding-bottom: 50rpx;
}
.pageBody button {
 padding: 0;
}
.list {
    width: 100%;
    /* border-bottom: 1rpx solid #e7e7e7; */
    padding:6rpx 0 30rpx 0;
}

.list:last-child {
    border-bottom: none;
}
.date {
    color: #666;
    font-size: 28rpx;
    padding-left: 10rpx;
}
.content {
    width: 100%;
    border-radius: 30rpx;
    background: url(https://page-bucket.oiaqye7985.com/wechat/mini-card/imgs/bg_card_list_01.png) no-repeat center;
    background-size: cover;
    margin-top: 28rpx;
    padding: 75rpx 50rpx;
    box-sizing: border-box;
    display: flex;
    flex-wrap: wrap;
    height: 410rpx;
    overflow: hidden;
    box-shadow: 0 0 20rpx #d2e6ff;
    position: relative;
}
.Shield{
  position: absolute;
  width: 100%;
  height: 100%;
  line-height: 410rpx;
  top: 0;
  left: 0;
  text-align: center;
  color: #fff;
  background: rgba(73, 62, 62, 0.5)
}
.content .leftContent {
    width: 70%;
    text-align: left;
}
.content .name {
    font-size: 34rpx;
    color: #fff;
    font-weight: 700;
}
.content .job{
    font-size: 28rpx;
    font-weight: 400;
    margin-left: 10rpx;
}
.content .company {
    font-size: 28rpx;
    color: #fff;
    margin-top: 15rpx;
    height: 20px;
    /* overflow: hidden; */
}
.content .line {
    width: 65rpx;
    height: 6rpx;
    background-color: #c5e8fd;
    margin-top: 40rpx;
}
.content .rightContent {
    width:120rpx;
    height:120rpx;
    border-radius: 100%;
    border: 2px solid #008ef9;
    overflow: hidden;
    text-align: right;
    margin-left: 20px;
}
.content .rightContent image {
    width: 100%;
    min-height: 120rpx;
}
.content .leftBottomContent {
    width: 70%;
    margin-top: -10rpx;
}
.content .rightBottomContent {
    width: 30%;
    height: 90rpx;
    margin-top: 60rpx;
    text-align: right;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items:center;
}
.content .rightBottomContent image {
    width: 170rpx;
}
.content .tel {
    color: #fff;
    font-size: 28rpx;
    margin-bottom: 15rpx;
}
.content .mail {
    color: #fff;
    font-size: 28rpx;
}
.contentSecond {
    background: url(https://page-bucket.oiaqye7985.com/wechat/mini-card/imgs/bg_card_list_02.png) no-repeat center;
    background-size: cover;
    position: relative;
}
.contentSecond .rightBottomContent {
    position: absolute;
    left: 50rpx;
    top: 0;
}
